    "#Transforma valor em N칰mero (intt)\n",
    "# limpeza inicial\n",
    "df_final[\"valor\"] = (\n",
    "    df_final[\"valor\"]\n",
    "    .str.replace(\"R$\", \"\", regex=False)   # remove R$\n",
    "    .str.replace(\".\", \"\", regex=False)    # remove separador de milhar\n",
    "    .str.replace(\",\", \".\", regex=False)   # troca v칤rgula por ponto\n",
    "    .str.strip()\n",
    ")\n",
    "\n",
    "# converte para n칰mero (mant칠m NaN nos inv치lidos)\n",
    "df_final[\"valor\"] = pd.to_numeric(df_final[\"valor\"], errors=\"coerce\")\n",
    "\n",
    "# converte para inteiro, removendo os NaN antes\n",
    "df_final[\"valor\"] = df_final[\"valor\"].dropna().astype(int)"
   ]
  },

    "                            quartos = int(match.group())\n",
    "                    elif \"vaga\" in info_lower:\n",
    "                        match = re.search(r\"\\d+\", info) ## Extrai apenas os n칰meros\n",
    "                        if match:\n",
    "                            vagas = int(match.group())\n",
    "                    elif \"banheiro\" in info_lower:\n",
    "                        match = re.search(r\"\\d+\", info) ## Extrai apenas os n칰meros\n",
    "                        if match:\n",
    "                            banheiros = int(match.group())\n",
    "\n",
